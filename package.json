{
  "name": "isap-smart-community-service",
  "version": "1.00.00",
  "frameworkversion": "1.07.09",
  "config": {
    "displayname": "iSap Smart Community Service"
  },
  "description": "iSap Smart Community Service v1.00.00",
  "author": "Val Liu",
  "license": "Private Copyright - for iSAP internal use only",
  "main": "test",
  "bin": "main.ts",
  "typings": "dist/index",
  "scripts": {
    "start": "cross-env NODE_ENV=production TS_NODE_CACHE=false npm run build:live",
    "debug": "cross-env NODE_ENV=development TS_NODE_CACHE=false npm run build:debug",
    "build": "ts-node main.ts",
    "build:live": "nodemon --restartcrash --exec ts-node main.ts",
    "build:debug": "nodemon --watch false --exec ts-node main.ts",
    "build:live:new": "nodemon --exec node -r ts-node/register main.ts",
    "build:live:new:debug": "nodemon --exec node --inspect -r ts-node/register main.ts",
    "clean": "ts-node ./shells/clean-cache.shell.ts",
    "wc": "find . -name '*.ts' -not -path './node_modules/*' -not -path './typings/*' | xargs wc -c",
    "test": "echo \"Error: no test specified\" && exit 1",
    "install-windows-service": "winser -i -s -a --startwithnpm --displayname \"%npm_package_config_displayname%\"",
    "uninstall-windows-service": "winser -r -x -s",
    "pack": "server-packager ./ --no-bytecode --out-path=./dist"
  },
  "nodemonConfig": {
    "ignore": [
      "**/.git/*",
      "**/.cache*/*",
      "**/logs/*",
      "**/node_modules/*",
      "**/files/*",
      "**/assets/*"
    ],
    "delay": "200"
  },
  "devDependencies": {},
  "dependencies": {
    "@types/bluebird": "3.5.22",
    "@types/mongodb": "3.1.26",
    "@types/node": "8.10.38",
    "@types/nodemailer": "4.6.2",
    "@types/parse": "2.4.7",
    "bcryptjs": "^2.4.3",
    "bluebird": "3.5.1",
    "body-parser": "^1.19.0",
    "caller": "1.0.1",
    "colors": "1.3.2",
    "cross-env": "^5.2.0",
    "express": "^4.17.1",
    "express-ws": "3.0.0",
    "express-ws-routes": "1.1.0",
    "getmac": "1.4.6",
    "json-prune": "1.1.0",
    "longjohn": "0.2.12",
    "mime-types": "2.1.19",
    "module-alias": "2.1.0",
    "mongodb": "3.2.1",
    "network": "0.3.2",
    "node-csv-parse": "1.2.0",
    "nodemailer": "4.6.8",
    "nodemon": "git+http://isap-dev-server:3000/iSAP-AP-Team/nodemon.git#dc821512",
    "npm": "git+http://isap-dev-server:3000/iSAP-AP-Team/npm.git#96bdc33",
    "parse-dashboard": "1.2.0",
    "parse-server": "git+http://isap-dev-server:3000/iSAP-AP-Team/parse-server.git#b7a4b7fa03",
    "qrcode": "^1.3.3",
    "request": "2.87.0",
    "rimraf": "^2.6.3",
    "rxjs": "5.5.11",
    "server-packager": "git+http://isap-dev-server:3000/iSAP-AP-Team/server-packager.git#e801a0aa52",
    "shortid": "^2.2.15",
    "tmp-promise": "^1.0.5",
    "ts-node": "git+http://isap-dev-server:3000/iSAP-AP-Team/ts-node.git#cbe90a614e",
    "ts-simple-ast": "16.0.1",
    "typescript": "3.1.1",
    "typings": "2.1.1",
    "uuid": "3.3.0",
    "websocket": "1.0.25",
    "winser": "git+http://isap-dev-server:3000/iSAP-AP-Team/winser.git#33b6fd8e35",
    "xml2js": "0.4.19"
  },
  "pkg": {
    "targets": [
      "node8-win-x64"
    ],
    "assets": [
      "**/*",
      "!workspace/config/**/*",
      "!workspace/custom/license/**/*",
      "!workspace/custom/files/**/*",
      "!dist/**/*",
      "!**/.git/**/*"
    ],
    "inplaces": [
      "workspace/config/**/*",
      "workspace/custom/license/**/*",
      "workspace/custom/files/**/*"
    ],
    "natives": {
      "helpers/sms/lib/sms-dll.dll": "./",
      "services/license/lib/windows/license_manager.node": "./",
      "services/face-detection/lib/LibFaceDetection.dll": "./"
    }
  }
}
