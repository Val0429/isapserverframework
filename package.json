    "body-parser": "1.17.1",
    "caller": "1.0.1",
    "colors": "1.3.2",
    "cross-env": "^5.2.0",
    "edge-js": "10.3.1",
    "express": "4.15.2",
    "express-ws": "3.0.0",
    "express-ws-routes": "1.1.0",
    "getmac": "1.4.6",
    "json-prune": "1.1.0",
    "longjohn": "0.2.12",
    "mime-types": "2.1.19",
    "module-alias": "2.1.0",
    "mongodb": "3.2.1",
    "network": "0.3.2",
    "node-csv-parse": "1.2.0",
    "nodemailer": "4.6.8",
    "nodemon": "1.17.5",
    "parse-dashboard": "1.2.0",
