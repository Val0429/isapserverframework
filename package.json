{
  "name": "isap-smartface-server",
  "version": "1.00.04",
  "frameworkversion": "1.08.14",
  "config": {
    "displayname": "iSap SmartFace Server"
  },
  "description": "iSap SmartFace Server v1.00.04",
  "author": "Val Liu",
  "license": "Private Copyright - for iSAP internal use only",
  "main": "test",
  "bin": "main.ts",
  "typings": "dist/index",
  "scripts": {
    "start": "npm run build:live",
    "debug": "npm run build:debug",
    "build": "ts-node main.ts",
    "build:live": "nodemon --restartcrash --exec ts-node main.ts",
    "build:debug": "nodemon --exec ts-node main.ts",
    "build:live:new": "nodemon --exec node -r ts-node/register main.ts",
    "build:live:new:debug": "nodemon --exec node --inspect --inspect-port=0 -r ts-node/register main.ts",
    "clean": "ts-node ./shells/clean-cache.shell.ts",
    "wc": "find . -name '*.ts' -not -path './node_modules/*' -not -path './typings/*' | xargs wc -c",
    "test": "echo \"Error: no test specified\" && exit 1",
    "install-windows-service": "winser -i -s -a --startwithnpm --displayname \"%npm_package_config_displayname%\"",
    "uninstall-windows-service": "winser -r -x -s",
    "pack": "ts-node ./core/packer.ts && server-packager ./ --no-bytecode --out-path=./dist",
    "dorelease": "ts-node ./shells/do-release.shell.ts -s"
  },
  "nodemonConfig": {
    "ignore": [
      "**/.git/*",
      "**/.cache*/*",
      "**/logs/*",
      "**/node_modules/*",
      "**/files/*",
      "**/assets/*"
    ],
    "delay": "200"
  },
  "devDependencies": {},
  "dependencies": {
    "@types/bluebird": "3.5.22",
    "@types/mongodb": "3.5.25",
    "@types/node": "12.12.6",
    "@types/nodemailer": "4.6.2",
    "@types/parse": "2.12.1",
    "bcryptjs": "^2.4.3",
    "bluebird": "3.5.1",
    "body-parser": "^1.19.0",
    "caller": "1.0.1",
    "colors": "1.3.2",
    "cross-env": "^5.2.0",
    "express": "^4.17.1",
    "express-ws": "3.0.0",
    "express-ws-routes": "1.1.0",
    "getmac": "1.4.6",
    "ipcheck": "^0.1.0",
    "json-prune": "1.1.0",
    "longjohn": "0.2.12",
    "mime-types": "2.1.19",
    "module-alias": "2.1.0",
    "mongodb": "^3.6.3",
    "nanoid": "^3.1.20",
    "network": "0.3.2",
    "node-csv-parse": "1.2.0",
    "nodemailer": "6.4.16",
    "nodemon": "git+http://isap-dev-server:3000/iSAP-AP-Team/nodemon.git#v1.00.02",
    "npm": "git+http://isap-dev-server:3000/iSAP-AP-Team/npm.git#v10.00.02",
    "parse-dashboard": "1.2.0",
    "parse-server": "^4.3.0",
    "qrcode": "^1.3.3",
    "request": "2.87.0",
    "rimraf": "^2.6.3",
    "rxjs": "5.5.11",
    "server-packager": "git+http://isap-dev-server:3000/iSAP-AP-Team/server-packager.git#v1.00.05",
    "sharp": "^0.26.2",
    "tmp-promise": "3.0.2",
    "ts-morph": "7.1.2",
    "ts-node": "git+http://isap-dev-server:3000/iSAP-AP-Team/ts-node.git#v10.00.02",
    "typescript": "3.8.3",
    "typings": "2.1.1",
    "uuid": "3.3.0",
    "websocket": "1.0.32",
    "winser": "git+http://isap-dev-server:3000/iSAP-AP-Team/winser.git#v1.00.04",
    "xml2js": "0.4.19",
    "yesno": "^0.3.1"
  },
  "pkg": {
    "targets": [
      "node12-win-x64"
    ],
    "assets": [
      "**/*",
      "!workspace/**/*",
      "!.setting_core.json",
      "!.setting_db.json",
      "!.setting_parseDashboard.json",
      "!.setting_parseServer.json",
      "!node_modules/winser/bin/nssm64.exe",
      "!node_modules/winser/bin/nssm.exe",
      "!dist/**/*",
      "!logs/**/*",
      "!**/.git/**/*"
    ],
    "inplaces": [
      ".setting_core.json",
      ".setting_db.json",
      ".setting_parseDashboard.json",
      ".setting_parseServer.json",
      "node_modules/winser/bin/nssm64.exe",
      "node_modules/winser/bin/nssm.exe"
    ]
  }
}