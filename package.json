{
  "name": "isap-vms-service",
  "version": "3.00.20",
  "frameworkversion": "1.07.02",
  "config": {
    "displayname": "iSap VMS Service"
  },
  "description": "iSap VMS Service v3.01.00",
  "author": "Val Liu",
  "license": "Private Copyright - for iSAP internal use only",
  "main": "test",
  "typings": "dist/index",
  "scripts": {
    "start": "npm run build:live",
    "debug": "cross-env NODE_ENV=development npm run build:live",
    "build": "ts-node main.ts",
    "build:live": "nodemon --exec ts-node main.ts",
    "build:live:new": "nodemon --exec node -r ts-node/register main.ts",
    "build:live:new:debug": "nodemon --exec node --inspect --inspect-port=0 -r ts-node/register main.ts",
    "clean": "ts-node ./shells/clean-cache.shell.ts",
    "wc": "find . -name '*.ts' -not -path './node_modules/*' -not -path './typings/*' | xargs wc -c",
    "test": "echo \"Error: no test specified\" && exit 1",
    "install-windows-service": "winser -i -s -a --startwithnpm --displayname \"%npm_package_config_displayname%\"",
    "uninstall-windows-service": "winser -r -x -s"
  },
  "nodemonConfig": {
    "ignore": [
      "**/.git/*",
      "**/.cache*/*",
      "**/logs/*",
      "**/node_modules/*",
      "**/files/*",
      "**/assets/*"
    ],
    "delay": "200"
  },
  "devDependencies": {},
  "dependencies": {
    "@types/bluebird": "3.5.22",
    "@types/mongodb": "3.1.26",
    "@types/node": "8.10.38",
    "@types/nodemailer": "4.6.2",
    "@types/parse": "2.4.7",
    "bcryptjs": "^2.4.3",
