{
  "name": "isap-smart-community-service",
  "version": "1.00.00",
  "frameworkversion": "2.03.00",
  "config": {
    "displayname": "iSap Smart Community Service"
  },
  "description": "iSap Smart Community Service v1.00.00",
  "author": "Val Liu",
  "license": "Private Copyright - for iSAP internal use only",
  "main": "test",
  "bin": "main.ts",
  "typings": "dist/index",
  "scripts": {
    "start": "cross-env NODE_ENV=production npm run build",
    "start:debug": "cross-env NODE_ENV=development npm run build:debug",
    "start:ts-node": "cross-env NODE_ENV=production npm run build:ts-node",
    "start:ts-node:debug": "cross-env NODE_ENV=development npm run build:ts-node:debug",
    "build": "nodemon --restartcrash --exec node --trace-warnings  -r ts-node/register/transpile-only main.ts",
    "build:debug": "nodemon --watch false --exec node --trace-warnings -r ts-node/register main.ts",
    "build:ts-node": "node --trace-warnings -r ts-node/register/transpile-only main.ts",
    "build:ts-node:debug": "node --trace-warnings -r ts-node/register main.ts",
    "clean": "ts-node ./shells/clean-cache.shell.ts"
  },
  "nodemonConfig": {
    "ignore": [
      "**/.git/*",
      "**/.cache*/*",
      "**/logs/*",
      "**/node_modules/*",
      "**/files/*",
      "**/assets/*"
    ],
    "delay": "200"
  },
  "devDependencies": {},
  "dependencies": {
    "@types/bluebird": "3.5.22",
    "@types/mongodb": "3.5.25",
    "@types/node": "12.12.6",
    "@types/nodemailer": "4.6.2",
    "@types/parse": "2.12.1",
    "bcryptjs": "^2.4.3",
    "bluebird": "3.5.1",
    "body-parser": "^1.19.0",
    "caller": "1.0.1",
    "colors": "1.3.2",
    "cross-env": "^5.2.0",
    "crypt-core": "^1.0.2",
    "express": "^4.17.1",
    "express-ws": "3.0.0",
    "express-ws-routes": "1.1.0",
    "getmac": "1.4.6",
    "json-prune": "1.1.0",
    "longjohn": "0.2.12",
    "mime-types": "2.1.19",
    "module-alias": "2.1.0",
    "mongodb": "3.5.9",
    "network": "0.3.2",
    "node-csv-parse": "1.2.0",
    "nodemon": "git+http://isap-dev-server:3000/iSAP-AP-Team/nodemon.git#dc821512",
    "parse-server": "^4.3.0",
    "qrcode": "^1.3.3",
    "request": "2.87.0",
    "rimraf": "^2.6.3",
    "rxjs": "5.5.11",
    "server-service-file": "1.2.0",
    "server-service-log": "1.1.2",
    "server-service-print": "1.3.5",
    "service-datetime": "1.6.1",
    "ts-morph": "7.1.2",
    "ts-node": "8.10.2",
    "typescript": "3.8.3",
    "typings": "2.1.1",
    "uuid": "3.3.0",
    "websocket": "1.0.25",
    "xml2js": "0.4.19"
  }
}
